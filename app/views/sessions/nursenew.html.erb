<h1>Nurse log in</h1>
	<%= form_tag do %>
	<div class="form-group">
		<div class="row">
			<div class="col-md-4">
				<%= label_tag :email %>
				<%= text_field_tag :email, "", :class=>'form-control' %>
			</div>
		</div>
	</div>
	
	<div class="form-group">
		<div class="row">
			<div class="col-md-4">
				<%= label_tag :password %>
				<%= password_field_tag :password, "", :class=>'form-control' %>
			</div>
		</div>
	</div> 
		<%= submit_tag "Log in", :class=>'btn btn-primary'%>
		
	<% end %>